<%include partials/header%>
    <form action="/users/login/admin" method="post" class="flex bg-white w-fit rounded-xl my-12 flex-col items-center justify-center container m-auto p-4">
        <p class="font-bold text-l">Admin Login</p>
        <%if(error){%>
            <small class="text-red-500">
        <%=error%>
      </small>
            <%}%>
                <input class="outline-none block bg-gray-200 pr-5 pl-3 py-1 border-b-3 mt-3 rounded-md" type="email" placeholder="Add email" name="email">
                <input class="outline-none block bg-gray-200 pr-5 pl-3 py-1 border-b-3 mt-3 rounded-md" type="password" placeholder="Add password" name="password">
                <input class="block py-1 px-5 bg-black rounded-2xl mt-4 text-white hover:text-gray-400 border-b-3 mt-3 rounded-md" type="submit" value="Register">

    </form>
    <%include partials/footer%>